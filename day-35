/*Q69: Find the largest element in an array.*/


#include <stdio.h>
int getSecondLargest(int arr[], int n) 
{
    int largest = -1, secondLargest = -1;

    for (int i = 0; i < n; i++) {
        if (arr[i] > largest) {
            secondLargest = largest;
            largest = arr[i];
        } else if (arr[i] < largest && arr[i] > secondLargest) 
        {
            secondLargest = arr[i];
        }
    }
    return secondLargest;
}
int main() 
{
    int arr[] = {10, 20, 30, 40, 50};
    int n = sizeof(arr) / sizeof(arr[0]);
    printf("%d\n", getSecondLargest(arr, n));
    return 0;


/*Q70: Rotate an array to right by k positions.*/

#include <stdio.h>
void rightRotate(int arr[], int n, int k) 
{
    k = k % n; // Normalize k
    int temp[n];
    for (int i = 0; i < n; i++) 
    {
        temp[(i + k) % n] = arr[i];
    }
    // Copy temp to arr
    for (int i = 0; i < n; i++) 
    {
        arr[i] = temp[i];
    }
}
int main() 
{
    int arr[] = {1, 2, 3, 4, 5};
    int n = 5, k = 2;
    rightRotate(arr, n, k);
    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);
    return 0;
}
